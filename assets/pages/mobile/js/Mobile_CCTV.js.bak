    $("#PreviewTV").on("click",function(){

    $("#PreviewTVplayer").html("");

    $("#PreviewTVModal").modal("show");
    $("#PreviewTVModal").find("#largeModalLabel").text("ช่องรายการตัวอย่างและข่าวสาคร");
